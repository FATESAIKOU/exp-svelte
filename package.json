{
  "name": "exp-svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
    "backend": "cd easy-backend && source venv/bin/activate && pip install -r requirements.txt && python3 app.py",
    "dev": "vite",
    "dev:msw": "VITE_USE_MSW=true npm run dev",
    "dev:backend": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run dev\"",
    "preview": "vite build && vite preview",
    "preview:msw": "VITE_USE_MSW=true npm run preview",
    "preview:backend": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run preview\"",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/svelte": "^8.1.11",
    "@storybook/svelte-vite": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.2.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.14.9",
    "concurrently": "^8.2.2",
    "jsdom": "^24.1.0",
    "msw": "^2.3.1",
    "storybook": "^8.1.11",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "tslib": "^2.6.3",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
